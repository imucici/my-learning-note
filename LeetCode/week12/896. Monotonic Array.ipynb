   "source": [
    "#題目:只要符合升冪或降冪，return True\n",
    "\n",
    "class Solution:\n",
    "    def isMonotonic(self, A: List[int]) -> bool:\n",
    "        s = sorted(A)\n",
    "        d = sorted(A,reverse = True)\n",
    "        if s == A or d == A:\n",
    "            return True\n",
    "        else:\n",
    "            return False"
   ]
  }
