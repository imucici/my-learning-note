    "class Solution:\n",
    "    def canConstruct(self, a: str, b: str) -> bool:\n",
    "        for i in set(a):\n",
    "            if i not in b:\n",
    "                return False\n",
    "            else:\n",
    "                if a.count(i) > b.count(i):\n",
    "                    return False\n",
    "        return True"
   ]
