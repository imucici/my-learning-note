   "source": [
    "class Solution:\n",
    "    def findOcurrences(self, text: str, a: str, b: str) -> List[str]:\n",
    "        l = []\n",
    "        t = text.split(\" \")\n",
    "\n",
    "        for i in range(len(t)-2):\n",
    "            if t[i] == a and t[i+1] == b:\n",
    "                l.append(t[i+2])\n",
    "        return l"
   ]
  }
