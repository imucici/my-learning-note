    "class Solution:\n",
    "    def subtractProductAndSum(self, n: int) -> int:\n",
    "        a = str(n)\n",
    "        m = 1\n",
    "        s = 0\n",
    "        for i in a :\n",
    "            m*=int(i)\n",
    "            s+=int(i)\n",
    "        final = m-s\n",
    "        return final"
   ]
  }
