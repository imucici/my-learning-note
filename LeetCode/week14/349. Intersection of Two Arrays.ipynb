   "source": [
    "class Solution:\n",
    "    def intersection(self, a: List[int], b: List[int]) -> List[int]:\n",
    "        a = set(a)\n",
    "        b = set(b)\n",
    "        lst = []\n",
    "        for i in a:\n",
    "            if i in b:\n",
    "                lst.append(i)\n",
    "        return lst"
   ]
  }
