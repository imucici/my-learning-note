   "source": [
    "class Solution:\n",
    "    def findMedianSortedArrays(self, n1, n2):\n",
    "        m = sorted(n1+n2)\n",
    "        if len(m)%2 != 0:\n",
    "            return m[len(m)//2]\n",
    "        else:\n",
    "            return (m[len(m)//2]+m[len(m)//2-1])/2"
   ]
  }
