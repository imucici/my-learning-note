    "class Solution:\n",
    "    def removeDuplicates(self, n):\n",
    "        for i in n :\n",
    "            count = n.count(i)\n",
    "            if count >2:\n",
    "                d = 0\n",
    "                while d < count-2:\n",
    "                    d+=1\n",
    "                    n.remove(i)"
   ]
  }
