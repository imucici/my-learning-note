   "source": [
    "class Solution:\n",
    "    def findDuplicates(self, nums: List[int]) -> List[int]:\n",
    "        s = sorted(nums)\n",
    "        a = []\n",
    "        for i in range(len(nums)-1):\n",
    "            if s[i] == s[i+1]:\n",
    "                a.append(s[i])\n",
    "        return a"
   ]
  }
