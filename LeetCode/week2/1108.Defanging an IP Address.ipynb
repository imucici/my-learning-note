   "source": [
    "# 題目:\n",
    "#將字串內的\".\"替換成\"[.]\"\n",
    "\n",
    "# 觀念:\n",
    "#replace('舊字符串','新字符串') 此函數適用於str\n",
    "#---------------------------------------------------------------------------\n",
    "\n",
    "class Solution:\n",
    "    def defangIPaddr(self, address: str) -> str:\n",
    "        return address.replace('.','[.]') # replace() 將字串內的\".\"替換成\"[.]\" "
   ]
  }
