    "# 題目:\n",
    "# 判斷整數n是否為2的次方\n",
    "\n",
    "class Solution:\n",
    "    def isPowerOfTwo(self, n) -> bool:                \n",
    "        if n <=0 :\n",
    "            print (False)\n",
    "        \n",
    "        while n %2 == 0 :\n",
    "            n/=2 \n",
    "    \n",
    "        if n == 1:\n",
    "            print (True)\n",
    "        else:\n",
    "            print (False)"
   ]
  }
