    "# 題目:\n",
    "# 判斷n是不是3的次方\n",
    "\n",
    "\n",
    "class Solution:\n",
    "    def isPowerOfThree(self, n: int):\n",
    "        if n == 1:\n",
    "            return True\n",
    "        elif n <= 0 or n%3 != 0:\n",
    "            return False\n",
    "        else:\n",
    "            return self.isPowerOfThree(n/3)"
   ]
  }
