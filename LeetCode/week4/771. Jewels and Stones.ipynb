   "source": [
    "# 題目:\n",
    "# 計算J中的元素在S出現幾次\n",
    "\n",
    "# J: str、S: str\n",
    "\n",
    "class Solution:\n",
    "    def numJewelsInStones(self,J,S) -> int: #判斷S是否在J裡面，是>寫入空list\n",
    "        a = []\n",
    "        j = list(J)\n",
    "        s = list(S)\n",
    "        for i in s:\n",
    "            if i in j:\n",
    "                a.append(i)\n",
    "        return len(a)"
   ]
  }
