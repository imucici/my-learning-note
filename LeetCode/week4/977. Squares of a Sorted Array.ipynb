    "# 題目:\n",
    "# 給定元素由小到大排序的list，返回元素平方的新list (由小到大)。\n",
    "\n",
    "# A: List[int]\n",
    "\n",
    "\n",
    "class Solution:\n",
    "    def sortedSquares(self, A):\n",
    "        list1 = []\n",
    "        for i in A:\n",
    "            list1.append(i**2)\n",
    "        return sorted(list1)"
   ]
  }
