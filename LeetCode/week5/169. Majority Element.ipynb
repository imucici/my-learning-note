    "# 題目:\n",
    "# 找出出現最多次的數\n",
    "# nums: List[int]\n",
    "\n",
    "class Solution:\n",
    "    def majorityElement(self, nums):\n",
    "        from collections import Counter\n",
    "        count = Counter(nums)\n",
    "        a = dict(count.most_common())\n",
    "        return max(a, key=a.get)"
   ]
  }
