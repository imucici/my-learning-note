    "# 題目:\n",
    "# 找缺失值(int)\n",
    "# nums: List[int]\n",
    "\n",
    "class Solution:\n",
    "    def missingNumber(self, nums):\n",
    "        n = len(nums)\n",
    "        return int((n+1)*n/2 - sum(nums)) #正確list值的總和 - nums總和 = 缺失值"
   ]
  }
