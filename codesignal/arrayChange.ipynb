    "def arrayChange(a):\n",
    "    count = 0\n",
    "    for i in range(len(a)-1):\n",
    "        if a[i+1]<=a[i]:\n",
    "            count = count + a[i] -a[i+1] + 1\n",
    "            a[i+1] = a[i] + 1\n",
    "    return count"
   ]
