    }
   ],
   "source": [
    "x"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 2, 3]\n",
      "[4, 5, 6]\n"
     ]
    }
   ],
   "source": [
    "#解壓 zip(*)\n",
    "a1,b1 = zip(*x) \n",
    "print(list(a1))\n",
    "print(list(b1))"
   ]
  }
